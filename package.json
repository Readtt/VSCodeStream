{
	"name": "vscodestream",
	"displayName": "VSCodeStream",
	"description": "Streams VSCode data live to a websocket, can also make requests to the users client.",
	"version": "0.0.1",
	"publisher": "HK",
	"repository": "https://github.com/Readtt/VSCodeStream",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "VSCodeStream",
			"properties": {
				"vscodestream.msUpdate": {
					"type": "number",
					"default": 100,
					"description": "Interval in which data gets sent to the websocket. Below 10ms is not recommended."
				},
				"vscodestream.port": {
					"type": "number",
					"default": 8999,
					"description": "Port for the websocket server."
				}
			}
		},
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.34.0",
		"@types/ws": "^7.4.2",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"express": "^4.17.1",
		"ws": "^7.4.5"
	}
}
